algoritmo "02ValorMaiorMenor"
var
   valor1, valor2 : real
inicio
   Escreva("Digite a valor 1: ")
   Leia(valor1)
   Escreva("Digite a valor 2: ")
   Leia(valor2)
   se valor1 > valor2 entao
      Escreval("O valor: ", valor1, " é maior que: ", valor2)
   senao
      Escreval("O valor: ", valor2, " é maior que: ", valor1)
   fimse
fimalgoritmo
