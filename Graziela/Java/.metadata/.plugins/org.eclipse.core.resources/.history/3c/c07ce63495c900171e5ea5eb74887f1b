import javax.swing.JOptionPane;

public class Exercicio3 {
	public static void main(String[] args) {
		float salario = Float.parseFloat(JOptionPane.showInputDialog("Digite o salário 1: "));
		salario += Float.parseFloat(JOptionPane.showInputDialog("Digite o salário 2: "));
		salario += Float.parseFloat(JOptionPane.showInputDialog("Digite o salário 3: "));
		salario += Float.parseFloat(JOptionPane.showInputDialog("Digite o salário 4: "));
		salario += Float.parseFloat(JOptionPane.showInputDialog("Digite o salário 5: "));
		float media = (float) (salario / 5.0);
		JOptionPane.showMessageDialog(null, "A média dos salários é: " + media);
	}
}